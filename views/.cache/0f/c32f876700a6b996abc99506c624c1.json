ion: fixed;\r\n  -webkit-font-smoothing: subpixel-antialiased;\r\n  width: 100%;\r\n\r\n  .z-header-inner {\r\n    position: relative;\r\n    display: flex;\r\n    width: 1000px;\r\n    height: 52px;\r\n    padding: 0 16px;\r\n    margin: 0 auto;\r\n    align-items: center;\r\n    transition: transform .3s;\r\n    justify-content: space-between;\r\n\r\n    .blog-name {\r\n      color: #0084ff;\r\n      font-size: 22px;\r\n    }\r\n\r\n    .input-wrapper {\r\n      background: #f6f6f6;\r\n      padding-left: 12px;\r\n      height: 34px;\r\n      padding: 4px 10px;\r\n      font-size: 14px;\r\n      border: 1px solid #ebebeb;\r\n      border-radius: 3px;\r\n      display: flex;\r\n      align-items: center;\r\n      width: 300px;\r\n      position: relative;\r\n\r\n      .input-s {\r\n        padding: 0;\r\n        overflow: hidden;\r\n        font-family: inherit;\r\n        font-size: inherit;\r\n        font-weight: inherit;\r\n        background: transparent;\r\n        border: none;\r\n        resize: none;\r\n        color: #121212;\r\n        height: 24px;\r\n        line-height: 24px;\r\n        display: inline-block;\r\n        vertical-align: middle;\r\n        width: 100%;\r\n        padding-right: 30px;\r\n\r\n        &:focus {\r\n          outline: none;\r\n        }\r\n      }\r\n\r\n      .search {\r\n        position: absolute;\r\n        width: 18px;\r\n        height: 18px;\r\n        right: 12px;\r\n        cursor: pointer;\r\n      }\r\n    }\r\n\r\n    .username-login {\r\n      cursor: pointer;\r\n      height: 52px;\r\n      line-height: 52px;\r\n      padding: 0 20px;\r\n      margin-right: -20px;\r\n\r\n      font {\r\n        display: inline-block;\r\n        height: 100%;\r\n      }\r\n    }\r\n\r\n    .user-wrapper {\r\n      width: 30px;\r\n      height: 30px;\r\n      cursor: pointer;\r\n\r\n      .user-icon {\r\n        width: 100%;\r\n        height: 100%;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n/*\r\n主体\r\n*/\r\n\r\n.content {\r\n  display: flex;\r\n  align-items: flex-start;\r\n  width: 1000px;\r\n  padding: 0 16px;\r\n  margin: 62px auto 10px auto;\r\n\r\n  .content-left {\r\n    flex-shrink: 0;\r\n    margin-right: 10px;\r\n    margin-bottom: 0;\r\n    width: 694px;\r\n\r\n    .content-left-inner {\r\n      box-shadow: 0 1px 3px rgba(18, 18, 18, .1);\r\n\r\n      .content-tab {\r\n        background: #fff;\r\n        border-bottom: 1px solid #f0f2f7;\r\n        display: flex;\r\n        height: 58px;\r\n\r\n        .content-tab-item {\r\n          align-items: center;\r\n          display: flex;\r\n          font-size: 16px;\r\n          margin: 0 22px;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  .content-right {\r\n    flex: 1;\r\n\r\n    .content-right-inner {\r\n      box-shadow: 0 1px 3px rgba(18, 18, 18, .1);\r\n\r\n      .action {\r\n        padding: 20px 15px;\r\n        border-bottom: 1px solid #f6f6f6;\r\n        background: #fff;\r\n        display: flex;\r\n\r\n        .action-item {\r\n          display: flex;\r\n          flex-direction: column;\r\n          justify-content: center;\r\n          align-items: center;\r\n          cursor: pointer;\r\n\r\n          .action-item-icon {\r\n            width: 32px;\r\n            height: 32px;\r\n\r\n            .action-icon {\r\n              width: 100%;\r\n              height: 100%;\r\n            }\r\n          }\r\n\r\n          .action-title {\r\n            font-size: 12px;\r\n            line-height: 1;\r\n            text-align: center;\r\n            color: #444;\r\n            margin-top: 12px;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\n/*\r\n文章列表\r\n*/\r\n#article-box {\r\n  .article-li {\r\n    border-bottom: 1px solid #f0f2f7;\r\n    padding: 20px;\r\n    background: #fff;\r\n\r\n    &:nth-last-child(1) {\r\n      border: 0;\r\n    }\r\n\r\n    .article-user {\r\n      color: #b2bac2;\r\n      margin-bottom: 10px;\r\n\r\n      .article-username {\r\n        margin-right: 10px;\r\n      }\r\n    }\r\n\r\n    .title-article {\r\n      line-height: 1.6;\r\n      color: #121212;\r\n      font-size: 18px;\r\n      font-weight: 600;\r\n      cursor: pointer;\r\n    }\r\n\r\n    .title-description {\r\n      margin-top: 9px;\r\n      max-height: 100px;\r\n      line-height: 1.67;\r\n      display: -webkit-box;\r\n      -webkit-line-clamp: 2;\r\n      overflow: hidden;\r\n      text-overflow: ellipsis;\r\n      -webkit-box-orient: vertical;\r\n      cursor: pointer;\r\n    }\r\n  }\r\n}\r\n\r\n\r\n//退出登录菜单\r\n.logout-wrapper {\r\n  position: fixed;\r\n  z-index: 100;\r\n  width: 120px;\r\n  box-shadow: 0 5px 20px rgba(18, 18, 18, .1);\r\n  background: #fff;\r\n  border: 1px solid #ebebeb;\r\n  border-radius: 4px;\r\n  font-size: 14px;\r\n  color: #8590a6;\r\n\r\n  .popover-arrow {\r\n    position: absolute;\r\n    left: 60px;\r\n    top: 0;\r\n    width: 32px;\r\n    height: 16px;\r\n    transform: translateX(-50%) translateY(-100%);\r\n    overflow: hidden;\r\n\r\n    &::after {\r\n      content: '';\r\n      position: absolute;\r\n      box-sizing: border-box;\r\n      width: 24px;\r\n      height: 24px;\r\n      border: 1px solid #ebebeb;\r\n      background: #fff;\r\n      transform: rotate(45deg);\r\n      bottom: -19px;\r\n    }\r\n  }\r\n\r\n  li {\r\n    cursor: pointer;\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"de72048279f47bf2d35f6dc1944826a8","cacheData":{"env":{}}}